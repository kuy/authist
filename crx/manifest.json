{
  "name": "Authist",
  "version": "0.0.0",
  "description": "Assists MFA/2FA",
  "content_scripts": [
    {
      "matches": [
        "https://*.signin.aws.amazon.com/*",
        "https://www.rust-lang.org/*"
      ],
      "js": ["dist/bundle.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["sites/*.js"],
  "permissions": ["nativeMessaging", "tabs", "activeTab", "https://*/*"],
  "manifest_version": 2
}
